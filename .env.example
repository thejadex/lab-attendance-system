# Environment Variables for Vercel Deployment
# Copy this file to .env for local development
# Set these as environment variables in Vercel dashboard

# Database URL (PostgreSQL)
# Format: postgresql://username:password@hostname:port/database_name
# Example: postgresql://user:pass@localhost:5432/attendance_db
DATABASE_URL=your_postgresql_database_url_here

# Secret key for Flask sessions (generate a random string)
SECRET_KEY=your-secret-key-here-change-this-to-random-string

# Flask Environment
FLASK_ENV=production